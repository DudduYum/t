{
  "name": "shinythingsjs",
  "version": "1.0.0",
  "description": "3d multiuser configurator ",
  "main": "server.js",
  "sideEffects": false,
  "scripts": {
    "dev": "webpack-dev-server --open --config build/webpack.dev.config.js",
    "adev": "webpack --watch --open --config build/webpack.dev.config.js",
    "build": "webpack --config build/webpack.prod.config.js",
    "start": "npx babel-node srv/server.js",
    "devstart": "npx nodemon --exec 'npm run start'",
    "tmp": "npx babel-node --experimental-modules srv/server.js",
    "help": "cat ./utils/man.txt"
  },
  "keywords": [
    "lance-gg",
    "aframe"
  ],
  "author": "Rostyslav Kostyuk (dudduyum)",
  "license": "ISC",
  "dependencies": {
    "aframe": "^0.9.2",
    "cannon": "^0.6.2",
    "core-js": "^3.2.1",
    "express": "^4.17.1",
    "jquery": "^3.4.1",
    "lance-gg": "^4.0.8",
    "net": "^1.0.2",
    "nodemon": "^1.19.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^2.0.2",
    "convert-source-map": "^1.6.0",
    "fs": "0.0.1-security",
    "html-webpack-plugin": "^3.2.0",
    "lowdash": "^1.2.0",
    "uws": "^100.0.1",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.1"
  }
}
